{
  "parser": "babel-eslint",
  "extends": ["prettier", "eslint:recommended"],
  "plugins": ["prettier"],
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "prettier/prettier": "error",
    "no-useless-escape": "off",
    "no-console": 0
  },
  "globals": {
    "module": true,
    "require": true,
    "process": true
  }
}
